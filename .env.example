# Artrio Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration (Required)
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Only needed for admin scripts (Optional)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_DB_URL=postgresql://postgres.your-project:password@aws-0-region.pooler.supabase.com:6543/postgres

# Application URLs (Optional - defaults shown)
VITE_APP_URL=http://localhost:5173

# Feature Flags (Optional - defaults shown)
VITE_ENABLE_ADMIN=true
VITE_ENABLE_ANALYTICS=false
